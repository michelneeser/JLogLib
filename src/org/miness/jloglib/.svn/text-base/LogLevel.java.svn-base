package org.miness.jloglib;

/**
 * Represents the three log levels LOW, MEDIUM and HIGH.
 * 
 * @author Michel Neeser
 * @version 1.0
 */
public enum LogLevel {

	/**
	 * The enum constant which represents the low log level.
	 */
	LOW,
	
	/**
	 * The enum constant which represents the medium log level.
	 */
	MEDIUM,
	
	/**
	 * The enum constant which represents the high log level.
	 */
	HIGH
	
}