package org.miness.jloglib;

/**
 * Represents the two log targets CONSOLE and FILE.
 * 
 * @author Michel Neeser
 * @version 1.0
 */
public enum LogTarget {

	/**
	 * The enum constant which represents the console log target.
	 */
	CONSOLE,
	
	/**
	 * The enum constant which represents the file log target.
	 */
	FILE
	
}